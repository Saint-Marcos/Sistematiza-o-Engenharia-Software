{
  "name": "Oops!.. I did it again",
  "version": "1.0",
  "description": "Extensão para gerenciar abas e limpar histórico",
  "manifest_version": 3,
  "permissions": ["storage", "tabs", "history"],
  "action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  }
}
